{
  "name": "l9studios",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hook-form": "^3.26.1",
    "react-meta-tags": "^0.7.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.3",
    "reactstrap": "^8.1.0"
  },
  "scripts": {
